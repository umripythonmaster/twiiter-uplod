{
    "version": 2,
    "builds": [
        {
            "src": "src/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "config": {
        "runtime": "python3.6"
    }
}
